---
- name: win_ping to windows powershell
  win_ping:
- name: run ipconfig
  win_command: ipconfig
  register: ipconfig
- debug: var=ipconfig
- win_shell: dir
  args:
    executable: cmd
  register: dir_out

- name: run ls in shell
  win_shell: ls
  register: ls_out
- debug: var=ls_out

- name: check winrm config
  win_shell: |
   winrm winrm/config
  register: winrm_out
- debug: var=winrm_out
